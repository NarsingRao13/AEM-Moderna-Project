<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header2"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <items
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <logo-title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Can not be empty"
                                        fieldLabel="Logo Title"
                                        name="./logoTitle"/>
                                    <navigation-bar
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Nav Data">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./navigationList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <nav-title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Can not be empty"
                                                            fieldLabel="Nav Title"
                                                            name="./navTitle"/>
                                                        <nav-path
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                            fieldDescription="Can not be empty"
                                                            fieldLabel="Nav Path"
                                                            name="./navPath"
                                                            rootPath="/content/Moderna/us/en"/>
                                                        <nav-type
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldDescription="Select Size"
                                                            fieldLabel="Nav-Type"
                                                            name="./navType">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <def
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Select"
                                                                    value=""/>
                                                                <normal
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Normal"
                                                                    value="normal"/>
                                                                <drop-down
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Drop-Down"
                                                                    value="dropDown"/>
                                                            </items>
                                                        </nav-type>
                                                        <Dropdown
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="DropDown">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./dropDownListData">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <column
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <dropdown-title
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldDescription="Can not be empty"
                                                                                fieldLabel="Dropdown Title"
                                                                                name="./dropDownTitle"/>
                                                                            <dropdown-path
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                fieldDescription="Can not be empty"
                                                                                fieldLabel="Path"
                                                                                name="./dropDownPath"
                                                                                rootPath="/content/Moderna/us/en"/>
                                                                            <deep-nav-type
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                                fieldDescription="Select Size"
                                                                                fieldLabel="Drop Down Type"
                                                                                name="./deepNavType">
                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                    <def
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Select"
                                                                                        value=""/>
                                                                                    <normal
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Normal"
                                                                                        value="normal"/>
                                                                                    <deep-drop-down
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Deep-Drop-Down"
                                                                                        value="deepDropDown"/>
                                                                                </items>
                                                                            </deep-nav-type>
                                                                            <DeepList
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                                composite="{Boolean}true"
                                                                                fieldLabel="Deep Drop Down">
                                                                                <field
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                                    name="./deepList">
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <column
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <deep-title
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                                    fieldDescription="Can not be empty"
                                                                                                    fieldLabel="Title"
                                                                                                    name="./deepDropDownTitle"/>
                                                                                                <deep-path
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                                    fieldDescription="Can not be empty"
                                                                                                    fieldLabel="Path"
                                                                                                    name="./deepDropDownPath"
                                                                                                    rootPath="/content/Moderna/us/en"/>
                                                                                            </items>
                                                                                        </column>
                                                                                    </items>
                                                                                </field>
                                                                            </DeepList>
                                                                        </items>
                                                                    </column>
                                                                </items>
                                                            </field>
                                                        </Dropdown>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </navigation-bar>
                                </items>
                            </column>
                        </items>
                    </columns>
                </items>
            </items>
        </items>
    </content>
</jcr:root>
