<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                type="nav">
                <items jcr:primaryType="nt:unstructured">
                    <footer-newsletter-tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Footer-Newsletter"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <newsletter-title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Newsletter Title"
                                                name="./newsletterTitle"/>
                                            <newsletter-description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Newsletter Description"
                                                name="./newsletterDescription"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footer-newsletter-tab>
                    <footer-top-tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Footer-Top"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <Tab1
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <links-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Mandatory Field"
                                                        fieldLabel="Links-Title"
                                                        name="./linksTitle"/>
                                                    <link1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Link1"
                                                        name="./link1"/>
                                                    <link2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Link2"
                                                        name="./link2"/>
                                                    <link3
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Link3"
                                                        name="./link3"/>
                                                    <link4
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Link4"
                                                        name="./link4"/>
                                                    <link5
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Link5"
                                                        name="./link5"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </Tab1>
                            <Tab2
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Services"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <service-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Mandatory Field"
                                                        fieldLabel="Service-Title"
                                                        name="./serviceTitle"/>
                                                    <service1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Service1"
                                                        name="./service1"/>
                                                    <service2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Service2"
                                                        name="./service2"/>
                                                    <service3
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Service3"
                                                        name="./service3"/>
                                                    <service4
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Service4"
                                                        name="./service4"/>
                                                    <service5
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Service5"
                                                        name="./service5"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </Tab2>
                            <Tab3
                                jcr:primaryType="nt:unstructured"
                                jcr:title="&#xa;Footer-Contact"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <contact-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Footer Contact Title"
                                                        name="./contactTitle"/>
                                                    <address-input1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Address-Input1"
                                                        name="./addressInput1"/>
                                                    <address-input2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Address-Input2"
                                                        name="./addressInput2"/>
                                                    <address-input3
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Address-Input3"
                                                        name="./addressInput3"/>
                                                    <phone-number
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Phone Number"
                                                        name="./contactPhnNum"/>
                                                    <email-id
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="Email-ID"
                                                        name="./contactEmailId"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </Tab3>
                            <Tab4
                                jcr:primaryType="nt:unstructured"
                                jcr:title="About Section"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <about-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="About-Title"
                                                        name="./aboutTitle"/>
                                                    <about-description
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldDescription="Can not be empty"
                                                        fieldLabel="About-Description"
                                                        name="./aboutDescription"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </Tab4>
                        </items>
                    </footer-top-tab>
                    <copyrights-tab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Copyrights"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <copyright-title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Copyright-Title"
                                                name="./copyrightTitle"/>
                                            <copyright-title-middel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Copyright-Title-Middel"
                                                name="./copyrightTitleMiddel"/>
                                            <copyright-title-last
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Copyright-Title-Last"
                                                name="./copyrightTitleLast"/>
                                            <credits-info
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Credits-Info"
                                                name="./creditsInfo"/>
                                            <credits-title-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mandatory Field"
                                                fieldLabel="Credits Title Link"
                                                name="./creditsTitleLink"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </copyrights-tab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
